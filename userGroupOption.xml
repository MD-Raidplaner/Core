<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/6.0/userGroupOption.xsd">
    <import>
        <categories>
            <category name="admin.rp">
                <parent>admin</parent>
            </category>
            <category name="admin.rp.character">
                <parent>admin.rp</parent>
            </category>
            <category name="admin.rp.raid">
                <parent>admin.rp</parent>
            </category>

            <category name="mod.rp">
                <parent>mod</parent>
            </category>
            <category name="mod.rp.calendar">
                <parent>mod.rp</parent>
            </category>
            <category name="mod.rp.raid">
                <parent>mod.rp</parent>
            </category>

            <category name="user.rp">
                <parent>user</parent>
            </category>
            <category name="user.rp.character">
                <parent>user.rp</parent>
            </category>
            <category name="user.rp.calendar">
                <parent>user.rp</parent>
            </category>
            <category name="user.rp.raid">
                <parent>user.rp</parent>
            </category>
        </categories>

        <options>
            <!-- admin.rp -->
            <option name="admin.rp.canManageGame">
                <categoryname>admin.rp</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <admindefaultvalue>1</admindefaultvalue>
                <usersonly>1</usersonly>
            </option>

            <!-- admin.rp.character -->
            <option name="admin.rp.canAddCharacter">
                <categoryname>admin.rp.character</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <admindefaultvalue>1</admindefaultvalue>
                <usersonly>1</usersonly>
            </option>
            <option name="admin.rp.canEditCharacter">
                <categoryname>admin.rp.character</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <admindefaultvalue>1</admindefaultvalue>
                <usersonly>1</usersonly>
            </option>
            <option name="admin.rp.canDeleteCharacter">
                <categoryname>admin.rp.character</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <admindefaultvalue>1</admindefaultvalue>
                <usersonly>1</usersonly>
            </option>
            <option name="admin.rp.canSearchCharacter">
                <categoryname>admin.rp.character</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <admindefaultvalue>1</admindefaultvalue>
                <usersonly>1</usersonly>
            </option>
            <option name="admin.rp.canEnableCharacter">
                <categoryname>admin.rp.character</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <admindefaultvalue>1</admindefaultvalue>
                <usersonly>1</usersonly>
            </option>
            <!-- /admin.rp.character -->

            <!-- admin.rp.raid -->
            <option name="admin.rp.canManagePointAccount">
                <categoryname>admin.rp.raid</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <admindefaultvalue>1</admindefaultvalue>
                <usersonly>1</usersonly>
            </option>
            <option name="admin.rp.canManageRaidEvent">
                <categoryname>admin.rp.raid</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <admindefaultvalue>1</admindefaultvalue>
                <usersonly>1</usersonly>
            </option>
            <!-- /admin.rp.raid -->

            <!-- mod.rp.calendar -->
            <option name="mod.rp.canEditEvent">
                <categoryname>mod.rp.calendar</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <moddefaultvalue>1</moddefaultvalue>
                <usersonly>1</usersonly>
            </option>
            <option name="mod.rp.canDeleteEvent">
                <categoryname>mod.rp.calendar</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <moddefaultvalue>1</moddefaultvalue>
                <usersonly>1</usersonly>
            </option>
            <option name="mod.rp.canRestoreEvent">
                <categoryname>mod.rp.calendar</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <moddefaultvalue>1</moddefaultvalue>
                <usersonly>1</usersonly>
            </option>
            <option name="mod.rp.canDeleteEventCompletely">
                <categoryname>mod.rp.calendar</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <moddefaultvalue>1</moddefaultvalue>
                <usersonly>1</usersonly>
            </option>
            <option name="mod.rp.canModerateEvent">
                <categoryname>mod.rp.calendar</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <moddefaultvalue>1</moddefaultvalue>
                <usersonly>1</usersonly>
            </option>
            <option name="mod.rp.canViewDeletedEvent">
                <categoryname>mod.rp.calendar</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <moddefaultvalue>1</moddefaultvalue>
                <usersonly>1</usersonly>
            </option>
			<option name="mod.rp.canEditEventComment">
				<categoryname>mod.rp.calendar</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<moddefaultvalue>1</moddefaultvalue>
				<usersonly>1</usersonly>
			</option>
			<option name="mod.rp.canDeleteEventComment">
				<categoryname>mod.rp.calendar</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<moddefaultvalue>1</moddefaultvalue>
				<usersonly>1</usersonly>
			</option>
			<option name="mod.rp.canModerateEventComment">
				<categoryname>mod.rp.calendar</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<moddefaultvalue>1</moddefaultvalue>
				<usersonly>1</usersonly>
			</option>
            <!-- /mod.rp.calendar -->

            <!-- mod.rp.raid -->
            <option name="mod.rp.canAddRaid">
                <categoryname>mod.rp.raid</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <moddefaultvalue>1</moddefaultvalue>
                <usersonly>1</usersonly>
            </option>
            <option name="mod.rp.canDeleteRaid">
                <categoryname>mod.rp.raid</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <moddefaultvalue>1</moddefaultvalue>
                <usersonly>1</usersonly>
            </option>
            <!-- /mod.rp.raid -->

            <!-- user.rp.character -->
            <option name="user.rp.canAddCharacter">
                <categoryname>user.rp.character</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>1</defaultvalue>
                <admindefaultvalue>1</admindefaultvalue>
                <usersonly>1</usersonly>
            </option>
            <option name="user.rp.canEditOwnCharacter">
                <categoryname>user.rp.character</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>1</defaultvalue>
                <admindefaultvalue>1</admindefaultvalue>
                <usersonly>1</usersonly>
            </option>
            <option name="user.rp.canDeleteOwnCharacter">
                <categoryname>user.rp.character</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>1</defaultvalue>
                <admindefaultvalue>1</admindefaultvalue>
                <usersonly>1</usersonly>
            </option>
            <option name="user.rp.canViewCharacterList">
                <categoryname>user.rp.character</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>1</defaultvalue>
            </option>
            <option name="user.rp.canViewCharacterProfile">
                <categoryname>user.rp.character</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>1</defaultvalue>
            </option>
            <option name="user.rp.canUploadCharacterAvatar">
                <categoryname>user.rp.character</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>1</defaultvalue>
                <enableoptions>user.rp.characterAvatarMaxSize,user.rp.characterAvatarAllowedFileExtensions</enableoptions>
            </option>
            <option name="user.rp.characterAvatarMaxSize">
                <categoryname>user.rp.character</categoryname>
                <optiontype>fileSize</optiontype>
                <defaultvalue>100000</defaultvalue>
                <minvalue>10000</minvalue>
                <usersonly>1</usersonly>
            </option>
            <option name="user.rp.characterAvatarAllowedFileExtensions">
                <categoryname>user.rp.character</categoryname>
                <optiontype>textarea</optiontype>
                <defaultvalue>gif
jpg
jpeg
png
webp</defaultvalue>
                <usersonly>1</usersonly>
            </option>
            <!-- /user.rp.character -->

            <!-- user.rp.calendar -->
            <option name="user.rp.canReadEvent">
                <categoryname>user.rp.calendar</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>1</defaultvalue>
            </option>
            <option name="user.rp.canCreateEvent">
                <categoryname>user.rp.calendar</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <moddefaultvalue>1</moddefaultvalue>
                <admindefaultvalue>1</admindefaultvalue>
                <enableoptions>user.rp.canCreateEventWithoutModeration</enableoptions>
                <usersonly>1</usersonly>
            </option>
            <option name="user.rp.canCreateEventWithoutModeration">
                <categoryname>user.rp.calendar</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <moddefaultvalue>1</moddefaultvalue>
                <admindefaultvalue>1</admindefaultvalue>
                <usersonly>1</usersonly>
            </option>
            <option name="user.rp.canEditOwnEvent">
                <categoryname>user.rp.calendar</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <moddefaultvalue>1</moddefaultvalue>
                <admindefaultvalue>1</admindefaultvalue>
                <usersonly>1</usersonly>
            </option>
            <option name="user.rp.canDeleteOwnEvent">
                <categoryname>user.rp.calendar</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <moddefaultvalue>1</moddefaultvalue>
                <usersonly>1</usersonly>
            </option>
			<option name="user.rp.canAddEventComment">
				<categoryname>user.rp.calendar</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<enableoptions>user.rp.canAddEventCommentWithoutModeration</enableoptions>
				<userdefaultvalue>1</userdefaultvalue>
				<excludedInTinyBuild>1</excludedInTinyBuild>
			</option>
			<option name="user.rp.canAddEventCommentWithoutModeration">
				<categoryname>user.rp.calendar</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<excludedInTinyBuild>1</excludedInTinyBuild>
			</option>
			<option name="user.rp.canEditEventComment">
				<categoryname>user.rp.calendar</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<usersonly>1</usersonly>
			</option>
			<option name="user.rp.canDeleteEventComment">
				<categoryname>user.rp.calendar</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<usersonly>1</usersonly>
			</option>
            <!-- /user.rp.calendar -->

            <!-- user.rp.raid -->
            <option name="user.rp.canParticipate">
                <categoryname>user.rp.raid</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <admindefaultvalue>1</admindefaultvalue>
            </option>
            <!-- /user.rp.raid -->
        </options>
    </import>
</data>