<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/6.0/clipboardAction.xsd">
    <import>
        <!-- de.md-raidplaner.rp.character -->
        <action name="enable">
            <actionclassname>rp\system\clipboard\action\CharacterClipboardAction</actionclassname>
            <showorder>1</showorder>
            <pages>
                <page>rp\acp\page\CharacterListPage</page>
            </pages>
        </action>
        <action name="delete">
            <actionclassname>rp\system\clipboard\action\CharacterClipboardAction</actionclassname>
            <showorder>2</showorder>
            <pages>
                <page>rp\acp\page\CharacterListPage</page>
            </pages>
        </action>
        <!-- /de.md-raidplaner.rp.character -->

        <!-- de.md-raidplaner.rp.raid.attendee -->
        <action name="updateStatus">
            <actionclassname>rp\system\clipboard\action\EventRaidAttendeeClipboardAction</actionclassname>
            <showorder>1</showorder>
            <pages>
                <page>rp\page\EventPage</page>
            </pages>
        </action>
        <action name="delete">
            <actionclassname>rp\system\clipboard\action\EventRaidAttendeeClipboardAction</actionclassname>
            <showorder>2</showorder>
            <pages>
                <page>rp\page\EventPage</page>
            </pages>
        </action>
        <!-- /de.md-raidplaner.rp.raid.attendee -->
    </import>
</data>